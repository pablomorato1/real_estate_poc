<div class="propertyPage_content">
  <ul class="nav nav-tabs" id="propertyTabs" role="tablist">
    <li class="nav-item" role="presentation">
      <button
        class="PropertyPage_tab nav-link"
        type="button"
        id="overview-tab"
        role="tab"
        data-bs-toggle="tab"
        data-bs-target="#overview-tab-pane"
        aria-controls="overview-tab-pane"
        aria-selected="false"
        tabindex="-1"
      >
        Property
      </button>
    </li>
    <li class="nav-item" role="presentation">
      <button
        class="PropertyPage_tab nav-link active"
        type="button"
        id="asset-tab"
        role="tab"
        data-bs-toggle="tab"
        data-bs-target="#asset-tab-pane"
        aria-controls="asset-tab-pane"
        aria-selected="true"
        tabindex="-1"
      >
        Portfolio
      </button>
    </li>
    <li class="nav-item" role="presentation">
      <button
        class="PropertyPage_tab nav-link"
        type="button"
        id="archive-tab"
        role="tab"
        data-bs-toggle="tab"
        data-bs-target="#archive-tab-pane"
        aria-controls="archive-tab-pane"
        aria-selected="false"
        tabindex="-1"
      >
        Archive
      </button>
    </li>
  </ul>
  <div class="tab-content">
    <div role="tabpanel" id="overview-tab-pane" aria-labelledby="overview-tab" class="fade tab-pane">
      <%= render partial: 'properties/overview' %>
    </div>
    <div role="tabpanel" id="asset-tab-pane" aria-labelledby="asset-tab" class="fade tab-pane active show">
      <%= render partial: 'properties/asset' %>
    </div>
    <div role="tabpanel" id="archive-tab-pane" aria-labelledby="archive-tab" class="fade tab-pane active show">
      <%= render partial: 'properties/archive' %>
    </div>
  </div>
</div>
